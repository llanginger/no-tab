{
  "manifest_version": 2,
  "name":  "Tabulatr",
  "description": "A tab management extension that unobtrusively stops you from accumulating zombie tabs!",
  "version": "1.12",

  "browser_action": {
    "default_icon": "images/tabulogo.png",
    "default_popup": "popup.html",
    "default_title": "Tab-U-Later"
  },

  "background": {
    "scripts": ["js/background.js"]
  },


  "permissions": [
    "tabs",
    "windows",
    "webNavigation",
    "storage"
  ]
}
